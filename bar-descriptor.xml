<?xml version='1.0' encoding='UTF-8' standalone='no'?>
<qnx xmlns="http://www.qnx.com/schemas/application/1.0">
    <id>com.iktwo.musicgear</id>
    <name>MusicGear</name>
    <versionNumber>0.0.1</versionNumber>
    <description>Stream and download your favorite music!</description>
    <initialWindow>
        <aspectRatio>portrait</aspectRatio>
        <autoOrients>false</autoOrients>
        <systemChrome>none</systemChrome>
        <transparent>false</transparent>
    </initialWindow>
    <author>Iktwo Corp.</author>
    <copyright>COPYRIGHT</copyright>
    <env var="QML2_IMPORT_PATH" value="app/native/imports"/>
    <env var="QT_PLUGIN_PATH" value="app/native/plugins"/>
    <env var="LD_LIBRARY_PATH" value="app/native/lib"/>
    <arg>-platform</arg>
    <arg>qnx</arg>
    <action system="true">run_native</action>
    <action>access_shared</action>
    <!-- musicgear should point to the project binary, path can be relative -->
    <asset type="Qnx/Elf" path="musicgear" entry="true">musicgear</asset>
    <asset path="%QT_INSTALL_PLUGINS%">plugins</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Core.so.5">lib/libQt5Core.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Gui.so.5">lib/libQt5Gui.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Network.so.5">lib/libQt5Network.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Multimedia.so.5">lib/libQt5Multimedia.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5MultimediaQuick_p.so.5">lib/libQt5MultimediaQuick_p.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Qml.so.5">lib/libQt5Qml.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Quick.so.5">lib/libQt5Quick.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Sql.so.5">lib/libQt5Sql.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5V8.so.5">lib/libQt5V8.so.5</asset>
    <asset path="%QT_INSTALL_LIBS%/libQt5Xml.so.5">lib/libQt5Xml.so.5</asset>
    <asset path="%QT_INSTALL_QML%/QtGraphicalEffects">imports/QtGraphicalEffects</asset>
    <asset path="%QT_INSTALL_QML%/QtMultimedia">imports/QtMultimedia</asset>
    <asset path="%QT_INSTALL_QML%/QtQuick.2">imports/QtQuick.2</asset>
    <asset path="%QT_INSTALL_QML%/QtQuick">imports/QtQuick</asset>
    <asset path="%QT_INSTALL_QML%/QtAudioEngine">imports/QtAudioEngine</asset>
    <asset path="%QT_INSTALL_QML%/Qt">imports/Qt</asset>
    <asset path="%SRC_DIR%/resources/images/icon_bb.png">icon</asset>
    <!-- These values will be replaced when deploying in Qt Creator -->
    <icon>
        <image>icon_bb.png</image>
    </icon>
    <authorId>gYAAgH43TNyknQZ4yVfW-mkq_i0</authorId>
</qnx>
